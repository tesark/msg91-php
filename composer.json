{
    "name": "venkatsamuthiram/deliver",
    "description": "Send SMS and OTP using MSG91 service",
    "keywords": ["sms and otp", "sms", "otp", "mobile sms", "mobile otp", "laravel sms", "laravel otp"],
    "type": "package",
    "license": "UNLICENSED",
    "require": {
        "guzzlehttp/guzzle": "~6.2",
        "propaganistas/laravel-phone": "dev-master",
        "monolog/monolog": "~1.13 ",
        "hassankhan/config": "dev-master"
    },
    "authors": [{
    	"name": "VenkaSkpi",
    	"email": "venkatsamuthiram5@gmail.com"
    }],
    "version": "1.0.0",
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0|^6.3@dev",
        "phpdocumentor/phpdocumentor": "2.*",
        "squizlabs/php_codesniffer": "^3.0@dev",
        "jakub-onderka/php-parallel-lint": "dev-master",
        "satooshi/php-coveralls": "^2.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Sender\\": "src/Sender"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sender\\": "tests/Sender"
        }
    },
    "scripts": {
        "test": [
            "parallel-lint . --exclude vendor",
            "phpunit -c phpunit.xml.dist --coverage-clover coverage.xml"
        ]
    }
}
